AIPR:CONT:
	BVER:UI32:2
	BTIM:UI64:1
	AINA:CONT:
#		"IP Range Name"
		AVER:UI32:2
		ATIM:UI64:1
		AVTP:FC32:'USTR'
		APER:FC32:'RDWR'
		AVAL:CONT:
			0x00000000:UI32:1
		END
	END	
	SIPA:CONT:
#		"IP Range"
		AVER:UI32:2
		ATIM:UI64:1
		AVTP:FC32:'CSTR'
		APER:FC32:'RDWR'
		AVAL:CONT:
			0x00000000:UI32:1
		END
	END
	LCNM:CONT:
#		"Location"
		AVER:UI32:2
		ATIM:UI64:1
		AVTP:FC32:'USTR'
		APER:FC32:'RDWR'
		AVAL:CONT:
			0x00000000:UI32:1
		END
	END
	DICM:CONT:
#		"DICOM"
		AVER:UI32:2
		ATIM:UI64:1
		AVTP:FC32:'ENUM'
		APER:FC32:'RDWR'
		AVAL:CONT:
			0x00000000:UI32:1
		END
	END
	HTTP:CONT:
#		"HTTP"
		AVER:UI32:2
		ATIM:UI64:1
		AVTP:FC32:'ENUM'
		APER:FC32:'RDWR'
		AVAL:CONT:
			0x00000000:UI32:1
		END
	END
END
