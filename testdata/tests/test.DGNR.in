DGNR:CONT:
	BVER:UI32:2
	BTIM:UI64:1
	DFAT:CONT:
#		"AE Title"
		AVER:UI32:2
		ATIM:UI64:1
		AVTP:FC32:'CSTR'
		APER:FC32:'RDWR'
		AVAL:CONT:
			0x00000000:UI32:2
			0x00000001:CSTR:"BYCAST-LDR"
		END
	END
	MPDU:CONT:
#		"Max PDU size"
		AVER:UI32:2
		ATIM:UI64:1
		AVTP:FC32:'UI32'
		APER:FC32:'RDWR'
		AVAL:CONT:
			0x00000000:UI32:2
			0x00000001:UI32:16384
		END
	END	
	DVLD:CONT:
#		"Validation Enable"
		AVER:UI32:2
		ATIM:UI64:1
		AVTP:FC32:'ENUM'
		APER:FC32:'RDWR'
		AVAL:CONT:
			0x00000000:UI32:2
			0x00000001:ENUM:0
		END
	END	
	DCMP:CONT:
#		"Compression Disable"
		AVER:UI32:2
		ATIM:UI64:1
		AVTP:FC32:'ENUM'
		APER:FC32:'RDWR'
		AVAL:CONT:
			0x00000000:UI32:2
			0x00000001:ENUM:0
		END
	END	
	DENC:CONT:
#		"Encryption Disable"
		AVER:UI32:2
		ATIM:UI64:1
		AVTP:FC32:'ENUM'
		APER:FC32:'RDWR'
		AVAL:CONT:
			0x00000000:UI32:2
			0x00000001:ENUM:0
		END
	END	
	ILQS:CONT:
#		"Image Level Query enable"
		AVER:UI32:2
		ATIM:UI64:1
		AVTP:FC32:'ENUM'
		APER:FC32:'RDWR'
		AVAL:CONT:
			0x00000000:UI32:2
			0x00000001:ENUM:1
		END
	END	
	ASTO:CONT:
#		"Association Sever Timeout"
		AVER:UI32:2
		ATIM:UI64:1
		AVTP:FC32:'UI32'
		APER:FC32:'RDWR'
		AVAL:CONT:
			0x00000000:UI32:2
			0x00000001:UI32:600000000
		END
	END	
	ACTO:CONT:
#		"Association Client Timeout"
		AVER:UI32:2
		ATIM:UI64:1
		AVTP:FC32:'UI32'
		APER:FC32:'RDWR'
		AVAL:CONT:
			0x00000000:UI32:2
			0x00000001:UI32:120000000
		END
	END
	MNCA:CONT:
#		"The default max number of concurrent associations"
		AVER:UI32:2
		ATIM:UI64:1
		AVTP:FC32:'UI32'
		APER:FC32:'RDWR'
		AVAL:CONT:
			0x00000000:UI32:2
			0x00000001:UI32:1
		END
	END
